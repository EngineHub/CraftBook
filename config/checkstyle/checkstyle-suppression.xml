<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <!-- This check only applies to package-info.java -->
    <suppress files="^((?!package-info\.java).)*$" id="package-info-NullMarked"/>
    <!-- Suppress NullMarked on tests for now -->
    <suppress files=".*[\\/]test[\\/].*\.java" checks="package-info-NullMarked|JavadocPackage"/>
    <!-- Suppress legacy files -->
    <suppress files="craftbook-bukkit[\\/]src[\\/]main[\\/]java[\\/]org[\\/]enginehub[\\/]craftbook[\\/]mechanics[\\/].*" checks=".*"/>
    <suppress files="craftbook-bukkit[\\/]src[\\/]main[\\/]java[\\/]org[\\/]enginehub[\\/]craftbook[\\/]util[\\/].*" checks=".*"/>
</suppressions>
